<h1>Listing encrypted_files</h1>

<table>
  <thead>
    <tr>
      <th>File</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @encrypted_files.each do |encrypted_file| %>
      <tr>
        <td><%= link_to encrypted_file.myfile_url, encrypted_file.myfile_url %></td>
        <td><%= link_to 'Show', encrypted_file %></td>
        <td><%= link_to 'Edit', edit_encrypted_file_path(encrypted_file) %></td>
        <td><%= link_to 'Destroy', encrypted_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

